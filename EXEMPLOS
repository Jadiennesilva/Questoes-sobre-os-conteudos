#include <stdio.h>
#include <locale.h>
#include <stdlib.h>


	
void  bubble_sort(int v[], int t){
	int aux;
	int troca =0;
	
	for(int i=0; i< t; i++){
		for(int j =0; j<t; j++){
			if(v[j]> v[j+1]){
				aux=v[i];
				v[i]=v[i+1];
				v[i+1]=aux;
				troca=1;
			}
			
		}
	} 
	return 0;
	
}
	 


