#include <stdio.h>
#include <locale.h>
#include <stdlib.h>

int main(){
	
void selection_sort(int num[], int tam){
	

int i, j, min, aux;

for(i=0; i < tam; i++){
	min=i;
	
	for(j = (i+1); j < tam; j++){
		if(num[j] < num[min]){
			min=j;
		}
	}
	
    if(i != min){
    	aux = num[i];
    	num[i] = num[min];
    	num[min] = aux;
    	
   }	
  }
 }


int main(){
	setlocale(LC_ALL,"portuguese_brazil");
	
	int 
}
	return 0;
}
